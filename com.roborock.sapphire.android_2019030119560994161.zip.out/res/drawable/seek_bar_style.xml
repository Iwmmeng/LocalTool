<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center_vertical" android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/volume_adjust_seek_bar_radius" />
            <solid android:color="#ffc9c7c7" />
        </shape>
    </item>
    <item android:gravity="center_vertical" android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/volume_adjust_seek_bar_radius" />
                <solid android:color="#ffc9c7c7" />
            </shape>
        </clip>
    </item>
    <item android:gravity="center_vertical" android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/volume_adjust_seek_bar_radius" />
                <solid android:color="#ff329ff8" />
            </shape>
        </clip>
    </item>
</layer-list>